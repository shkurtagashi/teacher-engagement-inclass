        android:layout_width="100dp"
        android:layout_height="38dp"
        android:layout_marginLeft="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:gravity="center_horizontal"
        android:text="@string/real_time_streaming"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintLeft_toLeftOf="@+id/real_time_streaming"
        app:layout_constraintRight_toRightOf="@+id/real_time_streaming"
        app:layout_constraintTop_toBottomOf="@+id/real_time_streaming" />

    <TextView
        android:id="@+id/textView16"
        android:layout_width="100dp"
        android:layout_height="38dp"
        android:layout_marginLeft="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
